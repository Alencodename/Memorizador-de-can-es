print ('''MEMORIZADOR DE CANÇÕES - CANÇÃO CARTIADA DAS ASAS ROTATIVAS
(POR ALENCA)''')
w = 0
A = 'Sulca o vento a espada que sustenta'
B = 'O rotativo versátil que encanta'
C = 'Colibri desafiando o espaço'
D = 'Baila, gira qual pássaro de aço'
E = 'O arqueiro alado se compraz'
F = 'Na amplitude do cíclico e coletivo'
G = 'Como artífice da guerra perspicaz'
H = 'Espreitando o perigo sempre altivo'
I = 'Aviação de asas rotativas'
J = 'Faz voo divino a pairar'
K = 'Aos rotores: o sabre'
L = 'O indelével sobejo dos ares'
M = 'Rompe o tempo ceifando o inimigo'
N = 'Com a espada helicoidal e sibilante'
O = 'Superando contínuos perigos'
P = 'Quer ativo camuflado ou rasante'
Q = 'Instrumento de guerra e de paz'
R = 'Na magnitude de vetor eficaz'
S = 'Desbravando inatingíveis cantões'
T = 'Reverbera sonoras ilusões'
U = 'Aviação de asas rotativas'
V = 'Faz voo divino a pairar'
W = 'Aos rotores: o sabre'
X = 'O indelével sobejo dos ares'
while w == 0:
    a = input('')
    w = w + 1
    if a == A:
        b = input ('')
        if b == B:
            c = input ('')
            if c == C:
                d = input ('')
                if d == D:
                    e = input ('')
                    if e == E:
                        f = input ('')
                        if f == F:
