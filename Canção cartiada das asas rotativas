                                                            input (N)
                                                            w = w * 0
                                                    else:
                                                        input (M)
                                                        w = w * 0
                                                else:
                                                    input (L)
                                                    w = w * 0
                                            else:
                                                input (K)
                                                w = w * 0
                                        else:
                                            input (J)
                                            w = w * 0
                                    else:
                                        input (I)
                                        w = w * 0
                                else:
                                    input (H)
                                    w = w * 0
                            else:
                                input (G)
                                w = w * 0
                        else:
                            input (F)
                            w = w * 0
                    else:
                        input (E)
                        w = w * 0
